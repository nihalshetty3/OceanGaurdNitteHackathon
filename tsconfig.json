{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["client/*"]
    },
    "types": ["vite/client", "node", "react", "react-dom"]
  },
  "include": [
    "client/**/*.ts",
    "client/**/*.tsx",
    "vite.config.ts"
  ]
}


